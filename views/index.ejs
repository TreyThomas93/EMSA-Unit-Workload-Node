<!-- SYSTEM CARDS -->
<div id="system-cards" class="container">
  <div id="level">
    <div>
      <h5>Level</h5>
    </div>
    <div>
      <h5 id="level-count"></h5>
    </div>
  </div>
  <div id="on-call">
    <div>
      <h5>On Call</h5>
    </div>
    <div>
      <h5 id="on-call-count"></h5>
    </div>
  </div>
  <div id="posting">
    <div>
      <h5>Posting</h5>
    </div>
    <div>
      <h5 id="posting-count"></h5>
    </div>
  </div>
  <div id="driving">
    <div>
      <h5>Driving</h5>
    </div>
    <div>
      <h5 id="driving-count"></h5>
    </div>
  </div>
  <div id="fueling">
    <div>
      <h5>Fueling</h5>
    </div>
    <div>
      <h5 id="fueling-count"></h5>
    </div>
  </div>
  <div id="eos">
    <div>
      <h5>EOS</h5>
    </div>
    <div>
      <h5 id="eos-count"></h5>
    </div>
  </div>
  <div id="late-call">
    <div>
      <h5>Late Call</h5>
    </div>
    <div>
      <h5 id="late-call-count"></h5>
    </div>
  </div>
  <div id="past-eos">
    <div>
      <h5>Past EOS</h5>
    </div>
    <div>
      <h5 id="past-eos-count"></h5>
    </div>
  </div>
</div>

<!-- WORKLOAD BAR CHART -->
<div id="workload-bar-chart-container" class="container outset-box-shadow">
  <canvas id="workload-bar-chart"></canvas>
</div>

<!-- WORKLOAD TABLE -->
<div id="workload-tables" class="container">
  <div id="legend" class="container">
    <label style="color: lightblue;">2 Hours (EOS)</label>
    <label style="color: lightgreen;">1 Hour (EOS)</label>
    <label style="color: red;">Past EOS</label>
  </div>
  <div id="inner-workload-tables">
    <div class="outset-box-shadow">
      <!-- ABOVE MAX -->
      <h5>Above Max ( <span id="total-above-max"></span> Units )</h5>
      <table id="above-max">
        <thead>
          <th>Unit</th>
          <th>Workload</th>
          <th>Threshold</th>
          <th></th>
        </thead>
        <tbody id="above-max-tbody"></tbody>
      </table>
    </div>
    <div class="outset-box-shadow">
      <!-- ABOVE CURRENT -->
      <h5>Above Current ( <span id="total-above-current"></span> Units )</h5>
      <table id="above-current">
        <thead>
          <th>Unit</th>
          <th>Workload</th>
          <th>Threshold</th>
          <th></th>
        </thead>
        <tbody id="above-current-tbody"></tbody>
      </table>
    </div>
    <div class="outset-box-shadow">
      <!-- OTHER -->
      <h5>Other ( <span id="total-other"></span> Units )</h5>
      <table id="other">
        <thead>
          <th>Unit</th>
          <th>Workload</th>
          <th>Threshold</th>
          <th></th>
        </thead>
        <tbody id="other-tbody"></tbody>
      </table>
    </div>
  </div>
</div>

<!-- CREW LIST -->
<div id="crew-list" class="container outset-box-shadow">
  <h5>Crew List (<span id="total-crews"></span> Total)</h5>
  <table>
    <thead>
      <th>Unit</th>
      <th>Crew Member One</th>
      <th>Crew Member Two</th>
    </thead>
    <tbody id="crew-list-tbody"></tbody>
  </table>
</div>

<!-- SYSTEM REPORT -->
<div id="system-report" class="container">
  <h5>System Report (<span id="status"></span>)</h5>

  <!-- SYSTEM REPORT GRAPHS -->
  <div id="system-report-graphs">
    <!-- off on time -->
    <div class="outset-box-shadow">
      <div>
        <h1 id="off-on-time"></h1>
        <h3>Off On Time</h3>
        <h4 id="daterange"></h4>
      </div>
    </div>

    <!-- log -->
    <div class="inset-box-shadow">
      <ul id="system-log"></ul>
    </div>

    <!-- units -->
    <div id="unit-chart-container" class="outset-box-shadow">
      <canvas id="unit-chart"></canvas>
    </div>

    <!-- calls -->
    <div id="calls-chart-container" class="outset-box-shadow">
      <canvas id="calls-chart"></canvas>
    </div>

    <!-- on call time -->
    <div id="on-call-time-chart-container" class="outset-box-shadow">
      <canvas id="on-call-time-chart"></canvas>
    </div>

    <!-- post time -->
    <div id="post-time-chart-container" class="outset-box-shadow">
      <canvas id="post-time-chart"></canvas>
    </div>

    <!-- drive time -->
    <div id="drive-time-chart-container" class="outset-box-shadow">
      <canvas id="drive-time-chart"></canvas>
    </div>

    <!-- task time -->
    <div id="task-time-chart-container" class="outset-box-shadow">
      <canvas id="task-time-chart"></canvas>
    </div>

    <!-- post assignments -->
    <div id="post-assignments-chart-container" class="outset-box-shadow">
      <canvas id="post-assignments-chart"></canvas>
    </div>

    <!-- events -->
    <div id="event-chart-container" class="outset-box-shadow">
      <canvas id="event-chart"></canvas>
    </div>
  </div>
</div>
